{
  "name": "uvu-expect-dom",
  "version": "0.2.1",
  "description": "jest-dom style assertions for uvu-expect",
  "main": "dist/cjs/index.cjs",
  "browser": "dist/esm/index.mjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "test": "uvu -r tsm -r global-jsdom/register tests -i common",
    "test:ci": "nyc pnpm test"
  },
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "license": "MIT",
  "homepage": "https://github.com/pablo-abc/uvu-expect-dom",
  "repository": "github:pablo-abc/uvu-expect-dom",
  "files": [
    "dist",
    "extend.js",
    "extend.mjs",
    "extend.cjs"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "@testing-library/dom": "^8.11.3",
    "@types/chai": "^4.3.0",
    "@types/css": "^0.0.33",
    "global-jsdom": "^8.4.0",
    "jsdom": "^19.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-ts": "^2.0.5",
    "tsm": "^2.2.1",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3",
    "uvu-expect": "^0.2.2"
  },
  "peerDependencies": {
    "uvu": "^0.5.3",
    "uvu-expect": "^0.2.2"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./extend": {
      "import": "./extend.mjs",
      "require": "./extend.cjs",
      "default": "./extend.mjs"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "css": "^3.0.0",
    "dom-accessibility-api": "^0.5.11",
    "redent": "^4.0.0"
  }
}
